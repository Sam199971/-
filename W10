        var x = 10,
            y = 20;
        var a = x + y,
            b = x - y,
            c = x * y,
            d = y / x,
            e = y % x;

        document.getElementById("aa").innerHTML = a + "<br>" + b + "<br>" + c + "<br>" + d + "<br>" + e;
        /*document.getElementById("aa").innerHTML = a + b+1;*/
    </script>
     
    <script>
        var x = 1,y = 5;
        var a = x + ++y, b = x + y++;
        document.getElementById("bb").innerHTML = y + "<br>" + a + "<br>" + b + "<br>" + y;

    </script>

    <script>
        var  a = 101010, b = 3;
        document.getElementById("cc").innerHTML = a << b;
    </script>

